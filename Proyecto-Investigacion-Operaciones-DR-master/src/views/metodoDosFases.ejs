    <!-- Contenido Principal -->
    <main class="py-4 flex-grow-1">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col">
                    <div class="text-center mb-3 miRojo">
                        <h1>Metodo Dos fases</h1><br>
                    </div>
                    <div class="row justify-content-center mb-3">
                        <label for="colFormLabel" class="col-sm-3 col-form-label">Digite la cantidad de
                            variables</label>
                        <div class="col-sm-2">
                            <input type="number" class="form-control" id="variables">
                        </div>
                    </div>
                    <div class="row justify-content-center mb-3">
                        <label for="colFormLabel" class="col-sm-3 col-form-label">Digite la cantidad de
                            restricciones</label>
                        <div class="col-sm-2">
                            <input type="number" class="form-control" id="restricciones">
                        </div>
                    </div>
                    <br>
                    <br>
                    <div class="d-grid gap-2 col-6 mx-auto">
                        <button id="botonAceptar" class="btn btn-lg mi-boton" type="button">Aceptar</button>
                    </div>
                    <div id="tituloFObjetivo" class="d-grid gap-2 col-6 mx-auto">
                    </div>
                    <math display="block">
                        <mtable class="funcionObjetivo" frame="solid" rowlines="solid" align="axis 3">
                        </mtable>
                    </math>
                    <div id="tituloRestriccion" class="d-grid gap-2 col-6 mx-auto">
                    </div>
                    <math display="block">
                        <mtable class="tablilla" frame="solid" rowlines="solid" align="axis 3">
                        </mtable>
                    </math>
                    <div id="divSolucionar" class="d-grid gap-2 col-6 mx-auto">
                    </div>
                </div>
            </div>
        </div>
    </main>


<body class="body-p d-flex flex-column h-100">

    <!-- Encabezado -->
    <header class="text-white py-4">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-auto">
                    <a id="logo-header" href="https://www.udistrital.edu.co/inicio" target="_blank">
